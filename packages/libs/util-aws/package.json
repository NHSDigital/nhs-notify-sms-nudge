{
  "dependencies": {
    "@aws-sdk/client-api-gateway": "3.821.0",
    "@aws-sdk/client-cloudwatch": "3.821.0",
    "@aws-sdk/client-cloudwatch-events": "3.821.0",
    "@aws-sdk/client-cloudwatch-logs": "3.821.0",
    "@aws-sdk/client-dynamodb": "3.821.0",
    "@aws-sdk/client-dynamodb-streams": "3.821.0",
    "@aws-sdk/client-lambda": "3.821.0",
    "@aws-sdk/client-pipes": "^3.613.0",
    "@aws-sdk/client-s3": "3.821.0",
    "@aws-sdk/client-scheduler": "3.821.0",
    "@aws-sdk/client-service-quotas": "3.817.0",
    "@aws-sdk/client-sqs": "3.821.0",
    "@aws-sdk/client-ssm": "3.821.0",
    "@aws-sdk/client-sts": "3.821.0",
    "@aws-sdk/lib-dynamodb": "3.821.0",
    "@aws-sdk/lib-storage": "3.821.0",
    "@aws-sdk/types": "3.821.0",
    "@aws-sdk/util-dynamodb": "3.821.0",
    "@redis/client": "^1.5.14",
    "@sms/util-logger": "*",
    "@sms/util-retry": "*",
    "@types/aws-lambda": "^8.10.119",
    "async-mutex": "^0.4.0",
    "fast-deep-equal": "3.1.3",
    "lodash": "^4.17.21",
    "p-limit": "3.1.0",
    "p-map": "4.0.0"
  },
  "devDependencies": {
    "@sms/config-jest": "*",
    "@sms/eslint-config-comms": "*",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.194",
    "@types/p-map": "^2.0.0",
    "aws-sdk-client-mock": "4.1.0",
    "aws-sdk-client-mock-jest": "4.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^29.3.1",
    "jest-allure": "^0.1.3",
    "jest-circus": "^29.2.2",
    "jest-junit": "^14.0.1",
    "jest-mock-extended": "^2.0.6",
    "typescript": "^4.8.4"
  },
  "eslintConfig": {
    "extends": [
      "@sms/eslint-config-comms/strict"
    ],
    "overrides": [
      {
        "files": [
          "*.test.*"
        ],
        "plugins": [
          "jest"
        ],
        "rules": {
          "@typescript-eslint/unbound-method": "off",
          "jest/unbound-method": "error"
        }
      }
    ],
    "rules": {
      "@typescript-eslint/unbound-method": "error"
    }
  },
  "main": "src/index.ts",
  "name": "@sms/util-aws",
  "private": true,
  "scripts": {
    "audit": "PACKAGE_DIR=$(pwd) pnpm --filter @comms/scripty-scripts run-audit",
    "fix": "pnpm prettier -w . --ignore-path ../../../.prettierignore",
    "lint": "pnpm lint:js && pnpm lint:fmt",
    "lint:fmt": "prettier --check . --ignore-path ../../../.prettierignore",
    "lint:js": "eslint .",
    "test:unit": "jest  --runInBand",
    "test:unit:coverage": "jest  --runInBand --coverage",
    "typecheck": "tsc --project ./tsconfig.json"
  },
  "sideEffects": false,
  "version": "0.0.1"
}
