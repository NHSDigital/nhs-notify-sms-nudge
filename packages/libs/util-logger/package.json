{
  "dependencies": {
    "@sms/config-jest": "*",
    "pino": "^8.6.1",
    "pino-pretty": "^4.8.0"
  },
  "devDependencies": {
    "@sms/eslint-config-comms": "*",
    "@types/jest": "^29.0.0",
    "@types/pino": "^7.0.5",
    "@types/pino-pretty": "^4.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.3.1",
    "jest-allure": "^0.1.3",
    "jest-circus": "^29.2.2",
    "jest-junit": "^14.0.1",
    "typescript": "^4.9.3"
  },
  "eslintConfig": {
    "extends": [
      "@sms/eslint-config-comms/strict"
    ]
  },
  "main": "src/index.ts",
  "name": "@sms/util-logger",
  "private": true,
  "scripts": {
    "audit": "PACKAGE_DIR=$(pwd) pnpm --filter @comms/scripty-scripts run-audit",
    "fix": "pnpm prettier -w . --ignore-path $(git rev-parse --show-toplevel)/.prettierignore && pnpm eslint --quiet --fix --no-error-on-unmatched-pattern .",
    "lint": "pnpm lint:js && pnpm lint:fmt",
    "lint:fmt": "prettier --check . --ignore-path ../../../.prettierignore",
    "lint:js": "eslint .",
    "test:unit": "jest  --runInBand",
    "test:unit:coverage": "jest  --runInBand --coverage",
    "typecheck": "tsc --project ./tsconfig.json"
  },
  "sideEffects": false,
  "version": "0.0.1"
}
